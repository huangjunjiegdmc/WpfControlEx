<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <!--#region Windows Commands-->

    <s:String x:Key="Themes">тем</s:String>
    <s:String x:Key="Minimize">минимизация</s:String>
    <s:String x:Key="Maximize">максимизация</s:String>
    <s:String x:Key="Restore">восстановление вниз</s:String>
    <s:String x:Key="Close">закрыть</s:String>

    <!--#endregion-->

    <!--#region Theme Menu-->

    <s:String x:Key="Blue">син</s:String>
    <s:String x:Key="Green">зелён</s:String>

    <!--#endregion-->

    <!--#region System Menus-->

    <s:String x:Key="Restore(R)">восстановление(&amp;R)</s:String>
    <s:String x:Key="Move(M)">перемещение(&amp;M)</s:String>
    <s:String x:Key="Size(S)">размер(&amp;S)</s:String>
    <s:String x:Key="Minimize(N)">минимизация(&amp;N)</s:String>
    <s:String x:Key="Maximize(X)">максимизация(&amp;X)</s:String>
    <s:String x:Key="Close(C)">закрыть(&amp;C)</s:String>

    <!--#endregion-->

    <!--#region MessageBox Commands-->

    <s:String x:Key="Confirm">определение</s:String>
    <s:String x:Key="Yes">Да</s:String>
    <s:String x:Key="No">Нет</s:String>
    <s:String x:Key="Cancel">отмена</s:String>

    <!--#endregion-->
    
</ResourceDictionary>