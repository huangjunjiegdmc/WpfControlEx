<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <!--#region Windows Commands-->

    <s:String x:Key="Minimize">最小化</s:String>
    <s:String x:Key="Maximize">最大化</s:String>
    <s:String x:Key="Restore">向下還原</s:String>
    <s:String x:Key="Close">關閉</s:String>

    <!--#endregion-->

    <!--#region System Menus-->

    <s:String x:Key="Restore(R)">還原(&amp;R)</s:String>
    <s:String x:Key="Move(M)">移動(&amp;M)</s:String>
    <s:String x:Key="Size(S)">大小(&amp;S)</s:String>
    <s:String x:Key="Minimize(N)">最小化(&amp;N)</s:String>
    <s:String x:Key="Maximize(X)">最大化(&amp;X)</s:String>
    <s:String x:Key="Close(C)">關閉(&amp;C)</s:String>

    <!--#endregion-->

    <!--#region MessageBox Commands-->

    <s:String x:Key="Confirm">確定</s:String>
    <s:String x:Key="Yes">是</s:String>
    <s:String x:Key="No">否</s:String>
    <s:String x:Key="Cancel">取消</s:String>

    <!--#endregion-->
    
</ResourceDictionary>