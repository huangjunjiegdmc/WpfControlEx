<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <!--#region Windows Commands-->

    <s:String x:Key="Minimize">Minimize</s:String>
    <s:String x:Key="Maximize">Maximize</s:String>
    <s:String x:Key="Restore">Restore</s:String>
    <s:String x:Key="Close">Close</s:String>

    <!--#endregion-->

    <!--#region System Menus-->

    <s:String x:Key="Restore(R)">Restore(&amp;R)</s:String>
    <s:String x:Key="Move(M)">Move(&amp;M)</s:String>
    <s:String x:Key="Size(S)">Size(&amp;S)</s:String>
    <s:String x:Key="Minimize(N)">Minimize(&amp;N)</s:String>
    <s:String x:Key="Maximize(X)">Maximize(&amp;X)</s:String>
    <s:String x:Key="Close(C)">Close(&amp;C)</s:String>

    <!--#endregion-->

</ResourceDictionary>